{
  "rewrites": [
    {
      "source": "/search",
      "destination": "/search/index.html"
    },
    {
      "source": "/search/",
      "destination": "/search/index.html"
    },
    {
      "source": "/search/:path*",
      "destination": "/search/index.html"  
    }
  ], 
  
  "redirects": [
   
    {
      "source": "/",
      "destination": "/?hl=tw",
      "permanent": true
    }
  ]
}
